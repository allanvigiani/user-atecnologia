{
  "expo": {
    "name": "AgendAi",
    "slug": "AgendAi",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/agendai_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/agendai_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/agendai_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.allanvigiani.AgendAi",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/agendai_logo.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "1181148c-c69c-451a-9678-5863dc451cb6"
      }
    }
  }
}
